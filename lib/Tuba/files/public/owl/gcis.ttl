	rdfs:range gcis:Agent ;
	rdfs:subPropertyOf prov:wasAttributedTo .

gcis:used a owl:ObjectProperty ;
	rdfs:label "Used" ;
	rdfs:comment "An activity uses entities when the activity is being undertaken." ;
	rdfs:subPropertyOf prov:used .

gcis:wasCreatedBy a owl:ObjectProperty ;
	rdfs:label "Was Created By" ;
	rdfs:comment "An image was created by an activity of image creation." ;
	rdfs:domain gcis:Image ;
	rdfs:range gcis:ImageCreation ;
	rdfs:subPropertyOf prov:wasGeneratedBy .

